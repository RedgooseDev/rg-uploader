    "type": "git",
    "url": "git+https://github.com/redgoose-dev/rg-uploader.git"
  },
  "keywords": [
    "uploader",
    "upload",
    "component",
    "file-uploader"
  ],
  "author": "redgoose <https://redgoose.me>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/redgoose-dev/rg-uploader/issues"
  },
  "homepage": "https://github.com/redgoose-dev/rg-uploader#readme",
  "dependencies": {
    "croppie": "2.6.4",
    "jquery": "3.4.1",
    "sortablejs": "1.10.2"
  },
  "devDependencies": {
    "css-loader": "3.4.2",
    "detect-file-type": "0.2.8",
    "express": "4.19.2",
    "file-loader": "6.0.0",
    "html-loader": "1.0.0",
    "html-webpack-plugin": "^4.0.2",
    "http-server": "0.12.1",
