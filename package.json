{
  "name": "rg-uploader",
  "version": "1.4.3",
  "main": "./src/rg-uploader.js",
  "scripts": {
    "dev": "./node_modules/.bin/webpack-dev-server --mode development --port 4000",
    "prebuild": "rm -rf dist && rm -rf docs/dist",
