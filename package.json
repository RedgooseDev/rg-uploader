    "multer": "1.4.2",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "query-string": "6.11.1",
    "sass": "1.26.3",
    "sass-loader": "8.0.2",
    "style-loader": "1.1.3",
    "terser-webpack-plugin": "2.3.5",
    "webpack": "4.42.1",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.10.3"
  }
